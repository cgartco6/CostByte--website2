            padding: 100px 0;
        }
        
        .course-card {
            transition: transform 0.3s, box-shadow 0.3s;
            border: none;
            border-radius: 10px;
            overflow: hidden;
            box-shadow: 0 5px 15px rgba(0, 0, 0, 0.1);
            margin-bottom: 20px;
            height: 100%;
        }
        
        .course-card:hover {
            transform: translateY(-10px);
            box-shadow: 0 15px 30px rgba(0, 0, 0, 0.15);
        }
        
        .level-badge {
            position: absolute;
            top: 15px;
            right: 15px;
            font-size: 0.8rem;
            padding: 5px 10px;
            border-radius: 30px;
        }
        
        .pricing {
            font-size: 1.8rem;
            font-weight: bold;
            color: var(--secondary);
        }
        
        .btn-enroll {
            background-color: var(--secondary);
            border: none;
            padding: 10px 25px;
            font-weight: 600;
            transition: all 0.3s;
        }
        
        .btn-enroll:hover {
            background-color: #c0392b;
            transform: scale(1.05);
        }
        
        .features-section {
