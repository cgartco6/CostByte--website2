    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
    <script>
        // Simple cart functionality
        document.addEventListener('DOMContentLoaded', function() {
            const enrollButtons = document.querySelectorAll('.btn-enroll');
            const cartCount = document.querySelector('#cart-count');
            const checkoutModal = new bootstrap.Modal(document.getElementById('checkoutModal'));
            let count = 0;
            let currentCourse = '';
            let currentPrice = '';
            
            enrollButtons.forEach(button => {
                button.addEventListener('click', function(e) {
                    e.preventDefault();
                    currentCourse = this.getAttribute('data-course');
                    currentPrice = 'R' + this.getAttribute('data-price');
                    
                    // Set modal content
                    document.getElementById('selected-course').textContent = currentCourse;
                    document.getElementById('course-name').textContent = currentCourse;
                    document.getElementById('course-price').textContent = currentPrice;
                    
                    // Show modal
                    checkoutModal.show();
                });
            });
            
            // Confirm enrollment
            document.getElementById('confirm-enroll').addEventListener('click', function() {
                count++;
                cartCount.textContent = count;
                
                // Hide modal
                checkoutModal.hide();
                
                // Show notification
                const alert = document.createElement('div');
                alert.className = 'alert alert-success position-fixed';
                alert.style.top = '20px';
                alert.style.right = '20px';
                alert.style.zIndex = '1050';
                alert.textContent = 'Successfully enrolled in ' + currentCourse + '!';
                document.body.appendChild(alert);
                
                // Remove alert after 3 seconds
                setTimeout(() => {
                    alert.remove();
                }, 3000);
            });
            
            // Contact form submission
            document.getElementById('contactForm').addEventListener('submit', function(e) {
                e.preventDefault();
                
                // Show notification
                const alert = document.createElement('div');
                alert.className = 'alert alert-success position-fixed';
                alert.style.top = '20px';
                alert.style.right = '20px';
                alert.style.zIndex = '1050';
                alert.textContent = 'Thank you for your message! We will get back to you soon.';
                document.body.appendChild(alert);
                
                // Remove alert after 3 seconds
                setTimeout(() => {
                    alert.remove();
                }, 3000);
                
                // Reset form
                this.reset();
            });
            
            // Newsletter subscription
            document.getElementById('newsletter-submit').addEventListener('click', function() {
                const email = document.getElementById('newsletter-email').value;
                
                if (email && isValidEmail(email)) {
                    // Show notification
                    const alert = document.createElement('div');
                    alert.className = 'alert alert-success position-fixed';
                    alert.style.top = '20px';
                    alert.style.right = '20px';
                    alert.style.zIndex = '1050';
                    alert.textContent = 'Thank you for subscribing to our newsletter!';
                    document.body.appendChild(alert);
                    
                    // Remove alert after 3 seconds
                    setTimeout(() => {
                        alert.remove();
                    }, 3000);
                    
                    // Reset input
                    document.getElementById('newsletter-email').value = '';
                } else {
                    alert('Please enter a valid email address.');
                }
            });
            
            // Update year in footer
            document.querySelector('footer .col-md-6 p').textContent = `Â© ${new Date().getFullYear()} Apex Trade AI Academy. All rights reserved.`;
            
            // Email validation
            function isValidEmail(email) {
                const re = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
                return re.test(email);
            }
        });
    </script>
